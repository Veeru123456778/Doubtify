{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/src/index.js"
    },
    {
      "source": "/temp/(.*)",
      "destination": "/public/temp/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
